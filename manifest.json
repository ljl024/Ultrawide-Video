{
    "name": "__MSG_extName__",
    "version": "1.7",
    "manifest_version": 2,
    "description": "__MSG_extDescription__",
    "icons": {
        "128": "images/icon.png"
    },
    "permissions": [
        "tabs",
        "storage",
        "notifications"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["Jquery/jquery-2.1.3.min.js","Jquery/jquery.hotkeys.js","Jquery/jquery.fullscreen.js","js/UltraWide.js"]
        }
    ],
    "background": {
        "scripts": ["js/background.js"]
    },
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_title": "__MSG_extName__",
        "default_popup":"html/popup.html"
    },
    "default_locale": "en"

}